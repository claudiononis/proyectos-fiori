            "request": "launch",
            "runtimeExecutable": "npx",
            "cwd": "${workspaceFolder}/ventilado",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--open",
                "test/flpSandbox.html#ventiladoptlventilado-display"
            ],
            "console": "internalConsole",
